<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="User Interface Compiler"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="TS"
			DisplayName="QT - Translation Release"
			CommandLine="%qtdir%\bin\lrelease de.ts -qm $(InputPath)\ts\$(InputName).qm"
			Outputs="$(ConfigurationName)\ts\$(InputName).qm"
			FileExtensions="*.ts"
			ExecutionDescription="Translation of $(InputName) is beeing processed..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
